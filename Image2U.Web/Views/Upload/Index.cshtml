<script async src="~/Scripts/Image2U/Upload.index.js"></script>
<div class="row">
    <div class="btn-group" role="group" aria-label="button group" style="margin: 10px 0 10px 0;">
        <button id="fileSelect" type="button" class="btn btn-default">Select Files</button>
        <button id="uploadFiles" type="button" class="btn btn-default">Upload Files</button>
    </div>
    <input id="fileElem"
           type="file"
           multiple
           accept="image/*"
           style="display: none" onchange="handleFiles(this.files)">
    <table id="fileSelectResult" class="table table-hover">
        <thead></thead>
        <tbody>
            <tr>
                <td style="height: 120px; background-color: lightgray; color: navy; text-align: center;vertical-align: middle">
                    <span>Please Select Files</span>
                </td>
            </tr>
        </tbody>
    </table>
</div>
